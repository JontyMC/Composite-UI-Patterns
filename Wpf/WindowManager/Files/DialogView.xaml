<UserControl x:Class="WindowManager.Files.DialogView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:cal="http://www.caliburnproject.org">
    <StackPanel>
        <TextBlock x:Name="Message" TextWrapping="Wrap" FontSize="14" />
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" Margin="0 8 0 0">
            <Button x:Name="Ok" Margin="2 0 0 0">
                <TextBlock FontSize="14">Ok</TextBlock>
            </Button>
            <Button x:Name="Cancel" Margin="2 0 0 0">
                <TextBlock FontSize="14">Cancel</TextBlock>
            </Button>
        </StackPanel>
    </StackPanel>
</UserControl>